recdate=$(date +%Y%m%d-%H%M)
mapdate=$(date '+%d %m %Y %H:%M')
timeout 378 /usr/local/bin/rtl_fm -d 0 -f 145.880M -s 48000 -g 44 -p 1 -F 9 -A fast -E DC ~/wxsat/recordings/FOX1-D-$recdate.raw
/usr/bin/sox -t raw -r 48000 -es -b16 -c1 -V1 ~/wxsat/recordings/FOX-1D-$recdate.raw ~/wxsat/recordings/FOX-1D-$recdate.wav rate 11025
touch -r ~/wxsat/recordings/FOX-1D-$recdate.raw ~/wxsat/recordings/FOX-1D-$recdate.wav
cp ~/wxsat/recordings/FOX-1D-$recdate.wav /media/cloud/coop/
